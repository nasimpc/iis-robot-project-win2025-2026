<?xml version="1.0"?>
<robot name="lab_room">
  <link name="floor">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry><box size="10 10 0.05"/></geometry>
      <material name="gray"><color rgba="0.7 0.7 0.7 1"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry><box size="10 10 0.05"/></geometry>
    </collision>
  </link>

  <link name="wall_n">
    <visual>
      <geometry><box size="10 0.1 1"/></geometry>
      <material name="white"><color rgba="0.9 0.9 0.9 1"/></material>
    </visual>
    <collision><geometry><box size="10 0.1 1"/></geometry></collision>
  </link>
  <joint name="j1" type="fixed">
    <parent link="floor"/><child link="wall_n"/><origin xyz="0 5 0.5"/>
  </joint>

  <link name="wall_s">
    <visual><geometry><box size="10 0.1 1"/></geometry></visual>
    <collision><geometry><box size="10 0.1 1"/></geometry></collision>
  </link>
  <joint name="j2" type="fixed">
    <parent link="floor"/><child link="wall_s"/><origin xyz="0 -5 0.5"/>
  </joint>

  <link name="wall_e">
    <visual><geometry><box size="0.1 10 1"/></geometry></visual>
    <collision><geometry><box size="0.1 10 1"/></geometry></collision>
  </link>
  <joint name="j3" type="fixed">
    <parent link="floor"/><child link="wall_e"/><origin xyz="5 0 0.5"/>
  </joint>

  <link name="wall_w">
    <visual><geometry><box size="0.1 10 1"/></geometry></visual>
    <collision><geometry><box size="0.1 10 1"/></geometry></collision>
  </link>
  <joint name="j4" type="fixed">
    <parent link="floor"/><child link="wall_w"/><origin xyz="-5 0 0.5"/>
  </joint>
</robot>
